[gd_scene format=3 uid="uid://b15hvbrf4cpfe"]

[sub_resource type="GDScript" id="GDScript_cksos"]
script/source = "extends Control

const SCENES := {
	\"kanban\": \"res://examples/kanban/main.tscn\",
	\"cube\": \"res://examples/cube/cube.tscn\",
	\"cookie\": \"res://examples/cookie_clicker/cookie_clicker.tscn\",
}

@onready var kanban_btn: Button = $Center/Buttons/KanbanButton
@onready var cube_btn: Button = $Center/Buttons/CubeButton
@onready var cookie_btn: Button = $Center/Buttons/CookieButton


func _ready() -> void:
	kanban_btn.pressed.connect(func(): get_tree().change_scene_to_file(SCENES[\"kanban\"]))
	cube_btn.pressed.connect(func(): get_tree().change_scene_to_file(SCENES[\"cube\"]))
	cookie_btn.pressed.connect(func(): get_tree().change_scene_to_file(SCENES[\"cookie\"]))


func _unhandled_input(event: InputEvent) -> void:
	if event.is_action_pressed(\"ui_cancel\"):
		get_tree().change_scene_to_file(\"res://examples/launcher/launcher.tscn\")
"

